@page
@model Seniorconnect_Luuk_deVos.Pages.Activity.ViewActivityModel
@{
    ViewData["Title"] = "ActiviteitBekijken";
}
<div>
    @if (Model.allusers != null && Model.allusers.Count > 0)
    {
        <h1>@Model.allusers.Count</h1>
        for (int i = 0; i < Model.allusers.Count; i++)
        {
            var user = Model.allusers[i];

          <p>@user.email</p>  
        }

        <a>@Model.activity.title</a>
        <br />
        <a>@Model.activity.description</a>
        <br />
        <a>@Model.activity.price</a>
        <br />
        <a>@Model.activity.startTime</a>
        <br />
        <a>@Model.activity.maxUsers</a>
    }
    </div>
